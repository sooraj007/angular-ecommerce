{
  "name": "angular-shop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --trace-uncaught ./node_modules/@angular/cli/bin/ng serve",
    "build": "ng build",
    "build:server": "tsc -p tsconfig.server.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:angular-shop": "node dist/angular-shop/server/server.mjs",
