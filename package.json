    "@angular-devkit/build-angular": "^19.0.7",
    "@angular/cli": "^19.0.7",
    "@angular/compiler-cli": "^19.0.0",
    "@spartan-ng/cli": "^0.0.1-alpha.380",
    "@types/better-sqlite3": "^7.6.12",
